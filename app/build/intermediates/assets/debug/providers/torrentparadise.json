{
  "searchUrl": "https://torrent-paradise.ml/api/search?q={query}",
  "listSelector": "div.result",
  "fields": {
    "title": { "selector": "a.title", "attr": "text" },
    "detailUrl": { "selector": "a.title", "attr": "href" },
    "seeders": { "selector": ".seeders", "attr": "text" },
    "leechers": { "selector": ".leechers", "attr": "text" },
    "size": { "selector": ".size", "attr": "text" }
  }
}
